@import "~style/common.styl"

.party-nav
  &__container
    display flex
    align-content center
    justify-content space-between
    padding rem(10) rem(35)
    height 45px
    background-color $white
    border-bottom 1px solid $black_lighter

  &__icon
    font-size rem(22) !important
    color $grey_medium

    &--active
      color $purple_medium